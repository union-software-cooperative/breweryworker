<span id="union">
  <% if @division %><a href="<%= polymorphic_path(union, division_id: @division.id) %>"><% end %>
    <% if union.logo.url.blank? %>
      <strong><%= union.short_name || union.name %></strong>
    <% else %>
      <%= logo_thumb(union) %>
    <% end %>
  <% if @division %></a><% end %>
</span>
